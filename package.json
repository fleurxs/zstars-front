{
  "name": "zstars-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "docker:up": "docker compose -f compose.yaml up -d",
    "docker:down": "docker compose -f compose.yaml down --remove-orphans",
    "docker:up:prod": "docker compose -f compose.prod.yaml up -d",
    "docker:down:prod": "docker compose -f compose.prod.yaml down --remove-orphans",
    "docker:build:prod": "docker compose -f compose.prod.yaml build",
    "docker:restart:prod": "docker compose -f compose.prod.yaml down --remove-orphans && docker compose -f compose.prod.yaml up -d"
  },
  "dependencies": {
    "lucide-react": "^0.556.0",
    "next": "16.*",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5"
  }
}
